}], "Text",
  FontWeight->"Plain",
  FontVariations->{"CompatibilityType"->0}],

Cell[BoxData[{
    \(\(lmax\  = \ 5;\)\[IndentingNewLine]\), "\[IndentingNewLine]", 
    \(\(alpha[l_, m_, j_]\  := \ 
        Sqrt[\((l - m)\)*\((l + m)\)*\((l - j)\)*\((l + j)\)/
              Max[1, l*l*\((4*l*l - 1)\)]];\)\), "\[IndentingNewLine]", 
    \(\(beta[l_, m_, j_]\  := \ 
        m*j/Max[1, l*\((l + 1)\)];\)\), "\[IndentingNewLine]", 
    \(\(P[l_, j_]\  := \ 
        p + l*\((l + 1)\) + eta*j*j + 
          i*T*j;\)\[IndentingNewLine]\), "\[IndentingNewLine]", 
    \(\(wminus = 
        Table[Table[
            Table[0, \ {j, \ \(-l\) - 1, \ l + 1}], \ {m, \ \(-l\) - 1, \ 
              l + 1}], \ {l, \ \(-1\), \ lmax}];\)\), "\[IndentingNewLine]", 
    \(\(wplus = 
        Table[Table[
            Table[0, \ {j, \ \(-l\), l}], \ {m, \ \(-l\), \ l}], \ {l, \ 0, \ 
            lmax + 1}];\)\), "\[IndentingNewLine]", 
    \(\(Wlm = 
        Table[Table[
            Table[0, \ {j, \ \(-l\), \ l}], \ {m, \ \(-l\), \ l}], \ {l, \ 
            0, \ lmax}];\)\[IndentingNewLine]\), "\[IndentingNewLine]", 
    \(For[m\  = \ \(-lmax\), \ m\  \[LessEqual] \ lmax, \ 
      For\ [j\  = \ \(-lmax\), \ j\  \[LessEqual] \ lmax, \ 
        wplus[\([lmax + 2, m + lmax + 2, j + lmax + 2]\)]\  = \ 
          1/\((\((lmax + 1)\)*\((lmax + 
                    2)\))\); \(j++\)]; \(m++\)]\), "\[IndentingNewLine]", 
    \(For[l\  = \ 0, \ l\  \[LessEqual] lmax, 
      For[m\  = \ \(-l\), \ m\  \[LessEqual] \ l, \ 
        For\ [j\  = \ \(-l\), \ j\  \[LessEqual] \ l, \ 
          wminus[\([l + 2, m + l + 2, j + l + 2]\)]\  = \ 
            1/\((P[l, \ j]\  - \ i*K*beta[l, \ m, \ j]\ \  + \ 
                  alpha[l, m, j]^2*K^2*
                    wminus[\([l + 1, m + l + 1, 
                        j + l + 
                          1]\)])\); \(j++\)]; \(m++\)]; \(l++\)]\), "\
\[IndentingNewLine]", 
    \(For[l\  = \ lmax, l \[GreaterEqual] 0, 
      For[m\  = \ \(-l\), \ m\  \[LessEqual] \ l, \ 
        For\ [j\  = \ \(-l\), \ j\  \[LessEqual] \ l, \ 
          wplus[\([l + 1, m + l + 1, j + l + 1]\)]\  = \ 
            1/\((P[l, \ j]\  - \ i*K*beta[l, \ m, \ j]\ \  + \ 
                  alpha[l + 1, m, j]^2*K^2*
                    wplus[\([l + 2, m + l + 2, 
                        j + l + 
                          2]\)])\); \(j++\)]; \(m++\)]; \(l--\)]\), "\
\[IndentingNewLine]", 
    \(For[l\  = \ 0, \ l\  \[LessEqual] lmax, 
      For[m\  = \ \(-l\), \ m\  \[LessEqual] \ l, \ 
        For\ [j\  = \ \(-l\), \ j\  \[LessEqual] \ l, \ 
          Wlm[\([l + 1, m + l + 1, j + l + 1]\)]\  = \ 
            1/\((P[l, \ j]\  - \ i*K*beta[l, \ m, \ j]\ \  + \ 
                  alpha[l, m, j]^2*K^2*
                    wminus[\([l + 1, m + l + 1, j + l + 1]\)]\  + \ 
                  alpha[l + 1, m, j]^2*K^2*
                    wplus[\([l + 2, m + l + 2, 
                        j + l + 
                          2]\)])\); \(j++\)]; \(m++\)]; \(l++\)]\), "\
\[IndentingNewLine]", 
